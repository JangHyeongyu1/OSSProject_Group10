#pragma once
#include <stdio.h>
#include <windows.h>
#include "struct.h"

void map()
{
    int i;
    for (i = 1; i < 40; i++)//x
    {
        gotoxy(i, 0);
        printf("■");
        gotoxy(i, 40);
        printf("■");
    }
    for (i = 0; i < 40; i++)//y
    {
        gotoxy(0, i);
        printf("■");
        gotoxy(40, i);
        printf("■");
    }
}
