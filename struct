#pragma once
#include<windows.h>

typedef struct {
	int x;
	int y;
	int check;
}FGrades;

typedef struct {
	int x;
	int y;
	int check;
}Meds;


typedef struct {
	int x;
	int y;
}Player;

void gotoxy(int x, int y)
{
	COORD pos = { x,y };
	SetConsoleCursorPosition(GetStdHandle(STD_OUTPUT_HANDLE), pos);
}
